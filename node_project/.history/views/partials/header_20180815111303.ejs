<header style="background-color: #07374D; height: 100px; display: flex; flex-direction: row; justify-content: space-between; flex-wrap: nowrap; width: 100%; z-index: 5; position: fixed;">
    <div style="display: flex; flex-direction: row;">
        <img src="images/sphynx_01.jpg" alt="Snow House Logo" style="border: 5px solid #07374D; height: 130px; border-radius: 50%; margin-top:6px; margin-left: 50px; z-index: 5;">
        <div class="title">
            <% let express = require('express'); %>
            <% let router = express.Router(); %>
            <% router.get('/breeds/:catID', (req, res) => { %>
            <% let dataFile = req.app.get("appData"); %>
            <% let cat = dataFile.breeds[req.params.catID]; %>
            <h5><%= cat.name %></h5>
            <% }) %>
            <p>Snow Ice  *  Tapioca  *  Smoothies</p>
        </div>
        </div>
        <div class="flexend">
        <a href="#">Home</a>
        <a href="menu.html">Menu</a>
        <a href="gallery.html">Gallery</a>
        <a href="contact.html">Contact<span class="hideit"> & Location</span></a>
    </div>
</header>